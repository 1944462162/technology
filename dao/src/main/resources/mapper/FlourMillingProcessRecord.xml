<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imustacm.dao.FlourMillingProcessRecordDao">

	<!-- flour_milling_process_record 所有查询列 -->
	<sql id="QUERY_COLUMN_LIST">
		<![CDATA[
            id,
            number,
            operator,
            create_time AS createTime,
            operation_of_equipment AS operationOfEquipment,
            feed_weight AS feedWeight,
            feed_instructions AS feedInstructions,
            milling_star_time AS millingStarTime,
            milling_end_time AS millingEndTime,
            milling_time AS millingTime,
            star_oxygen_content AS starOxygenContent,
            process_oxygen_content AS processOxygenContent,
            end_oxygen_content AS endOxygenContent,
            speed,
            grind_pressure AS grindPressure,
            milling_star_flow AS millingStarFlow,
            milling_end_flow AS millingEndFlow,
            material_weight AS materialWeight,
            spitting,
            yield,
            remarks 
		]]>
	</sql>

	<!-- flour_milling_process_record 查询列来源表-->
	<sql id="QUERY_FROM_TABLE"><![CDATA[FROM flour_milling_process_record]]></sql>
	
	<!-- 全部条件(更多功能可以通过queryData扩展实现)  -->
	<sql id="QUERY_WHERE_CLAUSE">
		<where>
            <if test="id != null and id != ''"><![CDATA[AND id = #{id}]]></if>
            <if test="number != null and number != ''"><![CDATA[AND number = #{number}]]></if>
            <if test="operator != null and operator != ''"><![CDATA[AND operator = #{operator}]]></if>
            <if test="createTime != null and createTime != ''"><![CDATA[AND create_time = #{createTime}]]></if>
            <if test="operationOfEquipment != null and operationOfEquipment != ''"><![CDATA[AND operation_of_equipment = #{operationOfEquipment}]]></if>
            <if test="feedWeight != null and feedWeight != ''"><![CDATA[AND feed_weight = #{feedWeight}]]></if>
            <if test="feedInstructions != null and feedInstructions != ''"><![CDATA[AND feed_instructions = #{feedInstructions}]]></if>
            <if test="millingStarTime != null and millingStarTime != ''"><![CDATA[AND milling_star_time = #{millingStarTime}]]></if>
            <if test="millingEndTime != null and millingEndTime != ''"><![CDATA[AND milling_end_time = #{millingEndTime}]]></if>
            <if test="millingTime != null and millingTime != ''"><![CDATA[AND milling_time = #{millingTime}]]></if>
            <if test="starOxygenContent != null and starOxygenContent != ''"><![CDATA[AND star_oxygen_content = #{starOxygenContent}]]></if>
            <if test="processOxygenContent != null and processOxygenContent != ''"><![CDATA[AND process_oxygen_content = #{processOxygenContent}]]></if>
            <if test="endOxygenContent != null and endOxygenContent != ''"><![CDATA[AND end_oxygen_content = #{endOxygenContent}]]></if>
            <if test="speed != null and speed != ''"><![CDATA[AND speed = #{speed}]]></if>
            <if test="grindPressure != null and grindPressure != ''"><![CDATA[AND grind_pressure = #{grindPressure}]]></if>
            <if test="millingStarFlow != null and millingStarFlow != ''"><![CDATA[AND milling_star_flow = #{millingStarFlow}]]></if>
            <if test="millingEndFlow != null and millingEndFlow != ''"><![CDATA[AND milling_end_flow = #{millingEndFlow}]]></if>
            <if test="materialWeight != null and materialWeight != ''"><![CDATA[AND material_weight = #{materialWeight}]]></if>
            <if test="spitting != null and spitting != ''"><![CDATA[AND spitting = #{spitting}]]></if>
            <if test="yield != null and yield != ''"><![CDATA[AND yield = #{yield}]]></if>
            <if test="remarks != null and remarks != ''"><![CDATA[AND remarks = #{remarks}]]></if> 
		</where>
	</sql>
	
	<!-- 智能排序与分页 -->
	<sql id="QUERY_ORDER_LIMIT_CONDTION">
		<if test="orderField != null and orderField != '' and orderFieldType != null and orderFieldType != ''"><![CDATA[ORDER BY ${orderField} ${orderFieldType}]]></if>
		<if test="startIndex != null and startIndex &gt;= 0 and pageSize != null and pageSize &gt; 0"><![CDATA[LIMIT #{startIndex},#{pageSize}]]></if>
	</sql>

	<!-- 更新列字段,只要不为NULL则更新,除开主键列 -->
	<sql id="UPDATE_COLUMN_SET">
		<set>
			<if test="number != null"><![CDATA[number = #{number},]]></if>
			<if test="operator != null"><![CDATA[operator = #{operator},]]></if>
			<if test="createTime != null"><![CDATA[create_time = #{createTime},]]></if>
			<if test="operationOfEquipment != null"><![CDATA[operation_of_equipment = #{operationOfEquipment},]]></if>
			<if test="feedWeight != null"><![CDATA[feed_weight = #{feedWeight},]]></if>
			<if test="feedInstructions != null"><![CDATA[feed_instructions = #{feedInstructions},]]></if>
			<if test="millingStarTime != null"><![CDATA[milling_star_time = #{millingStarTime},]]></if>
			<if test="millingEndTime != null"><![CDATA[milling_end_time = #{millingEndTime},]]></if>
			<if test="millingTime != null"><![CDATA[milling_time = #{millingTime},]]></if>
			<if test="starOxygenContent != null"><![CDATA[star_oxygen_content = #{starOxygenContent},]]></if>
			<if test="processOxygenContent != null"><![CDATA[process_oxygen_content = #{processOxygenContent},]]></if>
			<if test="endOxygenContent != null"><![CDATA[end_oxygen_content = #{endOxygenContent},]]></if>
			<if test="speed != null"><![CDATA[speed = #{speed},]]></if>
			<if test="grindPressure != null"><![CDATA[grind_pressure = #{grindPressure},]]></if>
			<if test="millingStarFlow != null"><![CDATA[milling_star_flow = #{millingStarFlow},]]></if>
			<if test="millingEndFlow != null"><![CDATA[milling_end_flow = #{millingEndFlow},]]></if>
			<if test="materialWeight != null"><![CDATA[material_weight = #{materialWeight},]]></if>
			<if test="spitting != null"><![CDATA[spitting = #{spitting},]]></if>
			<if test="yield != null"><![CDATA[yield = #{yield},]]></if>
			<if test="remarks != null"><![CDATA[remarks = #{remarks},]]></if>
		</set>
	</sql>

	<!-- 插入app记录 -->
	<insert id="insertEntry" parameterType="flourMillingProcessRecord" >
		<![CDATA[
			INSERT INTO flour_milling_process_record
				(
                id, 
                number, 
                operator, 
                create_time, 
                operation_of_equipment, 
                feed_weight, 
                feed_instructions, 
                milling_star_time, 
                milling_end_time, 
                milling_time, 
                star_oxygen_content, 
                process_oxygen_content, 
                end_oxygen_content, 
                speed, 
                grind_pressure, 
                milling_star_flow, 
                milling_end_flow, 
                material_weight, 
                spitting, 
                yield, 
                remarks 
				)
			VALUES
				(
                #{id}, 
                #{number}, 
                #{operator}, 
                #{createTime}, 
                #{operationOfEquipment}, 
                #{feedWeight}, 
                #{feedInstructions}, 
                #{millingStarTime}, 
                #{millingEndTime}, 
                #{millingTime}, 
                #{starOxygenContent}, 
                #{processOxygenContent}, 
                #{endOxygenContent}, 
                #{speed}, 
                #{grindPressure}, 
                #{millingStarFlow}, 
                #{millingEndFlow}, 
                #{materialWeight}, 
                #{spitting}, 
                #{yield}, 
                #{remarks} 
				)
		]]>
	</insert>
	
	<!-- 返回插入的编号,在事务开启状态下有效 -->
	<select id="lastSequence" resultType="long"><![CDATA[SELECT LAST_INSERT_ID() AS id]]></select>

	<!-- 删除记录,主键IN(array) -->
	<delete id="deleteByArrayKey" parameterType="java.lang.reflect.Array" >
		<![CDATA[DELETE FROM flour_milling_process_record WHERE id IN]]>
		<foreach collection="array" item="id" open="(" separator="," close=")">
			<![CDATA[#{id}]]>
		</foreach>
	</delete>

	<!-- 删除,通过条件 -->
	<delete id="deleteByCondtion" parameterType="flourMillingProcessRecord" >
		<![CDATA[DELETE FROM flour_milling_process_record]]>
		<include refid="QUERY_WHERE_CLAUSE"/>
	</delete>

	<!-- 修改记录通过主键 -->
	<update id="updateByKey" parameterType="flourMillingProcessRecord" >
		<![CDATA[UPDATE flour_milling_process_record]]>
		<include refid="UPDATE_COLUMN_SET"/>
		<![CDATA[WHERE id = #{id}]]>
	</update>

	<!-- 查询,通过主键IN(array) -->
	<select id="selectEntryArray" parameterType="java.lang.reflect.Array" resultType="flourMillingProcessRecord">
		<![CDATA[SELECT]]>
		<include refid="QUERY_COLUMN_LIST"/>
		<include refid="QUERY_FROM_TABLE"/>
		<![CDATA[WHERE id IN]]>
		<foreach collection="array" item="id" open="(" separator="," close=")">
			<![CDATA[#{id}]]>
		</foreach>
	</select>

	<!-- 查询,通过条件 -->
	<select id="selectEntryList" parameterType="flourMillingProcessRecord" resultType="flourMillingProcessRecord">
		<![CDATA[SELECT]]>
		<include refid="QUERY_COLUMN_LIST"/>
		<include refid="QUERY_FROM_TABLE"/>
		<include refid="QUERY_WHERE_CLAUSE"/>
		<include refid="QUERY_ORDER_LIMIT_CONDTION"/>
	</select>

	<!-- 总数查询,通过条件 -->
	<select id="selectEntryListCount" parameterType="flourMillingProcessRecord" resultType="int">
		<![CDATA[SELECT COUNT(id) AS dataCount]]>
		<include refid="QUERY_FROM_TABLE"/>
		<include refid="QUERY_WHERE_CLAUSE"/>
	</select>
	
	<!-- 其它SQL语句 -->
	
</mapper>