<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.imustacm.dao.PressureRecordDao">

	<!-- pressure_record 所有查询列 -->
	<sql id="QUERY_COLUMN_LIST">
		<![CDATA[
            id,
            number,
            press_number AS pressNumber,
            create_time AS createTime,
            roughcast_number AS roughcastNumber,
            picking,
            single_product AS singleProduct,
            tools_number AS toolsNumber,
            tools_size AS toolsSize,
            suppress_pressure AS suppressPressure,
            suppress_time AS suppressTime,
            magnetizing_current AS magnetizingCurrent,
            magentizing_time AS magentizingTime,
            demagnetization_current AS demagnetizationCurrent,
            demagnetizatio_time AS demagnetizatioTime,
            oxygen_weighing_room AS oxygenWeighingRoom,
            oxygen_packing_room AS oxygenPackingRoom,
            star_nitrogen AS starNitrogen,
            end_nitrogen AS endNitrogen,
            operator,
            remarks 
		]]>
	</sql>

	<!-- pressure_record 查询列来源表-->
	<sql id="QUERY_FROM_TABLE"><![CDATA[FROM pressure_record]]></sql>
	
	<!-- 全部条件(更多功能可以通过queryData扩展实现)  -->
	<sql id="QUERY_WHERE_CLAUSE">
		<where>
            <if test="id != null and id != ''"><![CDATA[AND id = #{id}]]></if>
            <if test="number != null and number != ''"><![CDATA[AND number = #{number}]]></if>
            <if test="pressNumber != null and pressNumber != ''"><![CDATA[AND press_number = #{pressNumber}]]></if>
            <if test="createTime != null and createTime != ''"><![CDATA[AND create_time = #{createTime}]]></if>
            <if test="roughcastNumber != null and roughcastNumber != ''"><![CDATA[AND roughcast_number = #{roughcastNumber}]]></if>
            <if test="picking != null and picking != ''"><![CDATA[AND picking = #{picking}]]></if>
            <if test="singleProduct != null and singleProduct != ''"><![CDATA[AND single_product = #{singleProduct}]]></if>
            <if test="toolsNumber != null and toolsNumber != ''"><![CDATA[AND tools_number = #{toolsNumber}]]></if>
            <if test="toolsSize != null and toolsSize != ''"><![CDATA[AND tools_size = #{toolsSize}]]></if>
            <if test="suppressPressure != null and suppressPressure != ''"><![CDATA[AND suppress_pressure = #{suppressPressure}]]></if>
            <if test="suppressTime != null and suppressTime != ''"><![CDATA[AND suppress_time = #{suppressTime}]]></if>
            <if test="magnetizingCurrent != null and magnetizingCurrent != ''"><![CDATA[AND magnetizing_current = #{magnetizingCurrent}]]></if>
            <if test="magentizingTime != null and magentizingTime != ''"><![CDATA[AND magentizing_time = #{magentizingTime}]]></if>
            <if test="demagnetizationCurrent != null and demagnetizationCurrent != ''"><![CDATA[AND demagnetization_current = #{demagnetizationCurrent}]]></if>
            <if test="demagnetizatioTime != null and demagnetizatioTime != ''"><![CDATA[AND demagnetizatio_time = #{demagnetizatioTime}]]></if>
            <if test="oxygenWeighingRoom != null and oxygenWeighingRoom != ''"><![CDATA[AND oxygen_weighing_room = #{oxygenWeighingRoom}]]></if>
            <if test="oxygenPackingRoom != null and oxygenPackingRoom != ''"><![CDATA[AND oxygen_packing_room = #{oxygenPackingRoom}]]></if>
            <if test="starNitrogen != null and starNitrogen != ''"><![CDATA[AND star_nitrogen = #{starNitrogen}]]></if>
            <if test="endNitrogen != null and endNitrogen != ''"><![CDATA[AND end_nitrogen = #{endNitrogen}]]></if>
            <if test="operator != null and operator != ''"><![CDATA[AND operator = #{operator}]]></if>
            <if test="remarks != null and remarks != ''"><![CDATA[AND remarks = #{remarks}]]></if> 
		</where>
	</sql>
	
	<!-- 智能排序与分页 -->
	<sql id="QUERY_ORDER_LIMIT_CONDTION">
		<if test="orderField != null and orderField != '' and orderFieldType != null and orderFieldType != ''"><![CDATA[ORDER BY ${orderField} ${orderFieldType}]]></if>
		<if test="startIndex != null and startIndex &gt;= 0 and pageSize != null and pageSize &gt; 0"><![CDATA[LIMIT #{startIndex},#{pageSize}]]></if>
	</sql>

	<!-- 更新列字段,只要不为NULL则更新,除开主键列 -->
	<sql id="UPDATE_COLUMN_SET">
		<set>
			<if test="number != null"><![CDATA[number = #{number},]]></if>
			<if test="pressNumber != null"><![CDATA[press_number = #{pressNumber},]]></if>
			<if test="createTime != null"><![CDATA[create_time = #{createTime},]]></if>
			<if test="roughcastNumber != null"><![CDATA[roughcast_number = #{roughcastNumber},]]></if>
			<if test="picking != null"><![CDATA[picking = #{picking},]]></if>
			<if test="singleProduct != null"><![CDATA[single_product = #{singleProduct},]]></if>
			<if test="toolsNumber != null"><![CDATA[tools_number = #{toolsNumber},]]></if>
			<if test="toolsSize != null"><![CDATA[tools_size = #{toolsSize},]]></if>
			<if test="suppressPressure != null"><![CDATA[suppress_pressure = #{suppressPressure},]]></if>
			<if test="suppressTime != null"><![CDATA[suppress_time = #{suppressTime},]]></if>
			<if test="magnetizingCurrent != null"><![CDATA[magnetizing_current = #{magnetizingCurrent},]]></if>
			<if test="magentizingTime != null"><![CDATA[magentizing_time = #{magentizingTime},]]></if>
			<if test="demagnetizationCurrent != null"><![CDATA[demagnetization_current = #{demagnetizationCurrent},]]></if>
			<if test="demagnetizatioTime != null"><![CDATA[demagnetizatio_time = #{demagnetizatioTime},]]></if>
			<if test="oxygenWeighingRoom != null"><![CDATA[oxygen_weighing_room = #{oxygenWeighingRoom},]]></if>
			<if test="oxygenPackingRoom != null"><![CDATA[oxygen_packing_room = #{oxygenPackingRoom},]]></if>
			<if test="starNitrogen != null"><![CDATA[star_nitrogen = #{starNitrogen},]]></if>
			<if test="endNitrogen != null"><![CDATA[end_nitrogen = #{endNitrogen},]]></if>
			<if test="operator != null"><![CDATA[operator = #{operator},]]></if>
			<if test="remarks != null"><![CDATA[remarks = #{remarks},]]></if>
		</set>
	</sql>

	<!-- 插入app记录 -->
	<insert id="insertEntry" parameterType="pressureRecord" >
		<![CDATA[
			INSERT INTO pressure_record
				(
                id, 
                number, 
                press_number, 
                create_time, 
                roughcast_number, 
                picking, 
                single_product, 
                tools_number, 
                tools_size, 
                suppress_pressure, 
                suppress_time, 
                magnetizing_current, 
                magentizing_time, 
                demagnetization_current, 
                demagnetizatio_time, 
                oxygen_weighing_room, 
                oxygen_packing_room, 
                star_nitrogen, 
                end_nitrogen, 
                operator, 
                remarks 
				)
			VALUES
				(
                #{id}, 
                #{number}, 
                #{pressNumber}, 
                #{createTime}, 
                #{roughcastNumber}, 
                #{picking}, 
                #{singleProduct}, 
                #{toolsNumber}, 
                #{toolsSize}, 
                #{suppressPressure}, 
                #{suppressTime}, 
                #{magnetizingCurrent}, 
                #{magentizingTime}, 
                #{demagnetizationCurrent}, 
                #{demagnetizatioTime}, 
                #{oxygenWeighingRoom}, 
                #{oxygenPackingRoom}, 
                #{starNitrogen}, 
                #{endNitrogen}, 
                #{operator}, 
                #{remarks} 
				)
		]]>
	</insert>
	
	<!-- 返回插入的编号,在事务开启状态下有效 -->
	<select id="lastSequence" resultType="long"><![CDATA[SELECT LAST_INSERT_ID() AS id]]></select>

	<!-- 删除记录,主键IN(array) -->
	<delete id="deleteByArrayKey" parameterType="java.lang.reflect.Array" >
		<![CDATA[DELETE FROM pressure_record WHERE id IN]]>
		<foreach collection="array" item="id" open="(" separator="," close=")">
			<![CDATA[#{id}]]>
		</foreach>
	</delete>

	<!-- 删除,通过条件 -->
	<delete id="deleteByCondtion" parameterType="pressureRecord" >
		<![CDATA[DELETE FROM pressure_record]]>
		<include refid="QUERY_WHERE_CLAUSE"/>
	</delete>

	<!-- 修改记录通过主键 -->
	<update id="updateByKey" parameterType="pressureRecord" >
		<![CDATA[UPDATE pressure_record]]>
		<include refid="UPDATE_COLUMN_SET"/>
		<![CDATA[WHERE id = #{id}]]>
	</update>

	<!-- 查询,通过主键IN(array) -->
	<select id="selectEntryArray" parameterType="java.lang.reflect.Array" resultType="pressureRecord">
		<![CDATA[SELECT]]>
		<include refid="QUERY_COLUMN_LIST"/>
		<include refid="QUERY_FROM_TABLE"/>
		<![CDATA[WHERE id IN]]>
		<foreach collection="array" item="id" open="(" separator="," close=")">
			<![CDATA[#{id}]]>
		</foreach>
	</select>

	<!-- 查询,通过条件 -->
	<select id="selectEntryList" parameterType="pressureRecord" resultType="pressureRecord">
		<![CDATA[SELECT]]>
		<include refid="QUERY_COLUMN_LIST"/>
		<include refid="QUERY_FROM_TABLE"/>
		<include refid="QUERY_WHERE_CLAUSE"/>
		<include refid="QUERY_ORDER_LIMIT_CONDTION"/>
	</select>

	<!-- 总数查询,通过条件 -->
	<select id="selectEntryListCount" parameterType="pressureRecord" resultType="int">
		<![CDATA[SELECT COUNT(id) AS dataCount]]>
		<include refid="QUERY_FROM_TABLE"/>
		<include refid="QUERY_WHERE_CLAUSE"/>
	</select>
	
	<!-- 其它SQL语句 -->
	
</mapper>