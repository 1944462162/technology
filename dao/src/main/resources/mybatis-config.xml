<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <typeAliases>
        <!--这里给实体类取别名，方便在mapper配置文件使用-->
        <typeAlias type="com.imustacm.domain.Po.AgglomerationProcessRecord" alias="agglomerationProcessRecord"/>
        <typeAlias type="com.imustacm.domain.Po.FlourMillingProcessRecord" alias="flourMillingProcessRecord"/>
        <typeAlias type="com.imustacm.domain.Po.PressureRecord" alias="pressureRecord"/>
        <typeAlias type="com.imustacm.domain.Po.Product" alias="product"/>
        <typeAlias type="com.imustacm.domain.Po.Roles" alias="roles"/>
        <typeAlias type="com.imustacm.domain.Po.RolesPermission" alias="rolesPermission"/>
        <typeAlias type="com.imustacm.domain.Po.Smelt" alias="smelt"/>
        <typeAlias type="com.imustacm.domain.Po.SmeltingRecord" alias="smeltingRecord"/>
        <typeAlias type="com.imustacm.domain.Po.SpareParts" alias="spareParts"/>
        <typeAlias type="com.imustacm.domain.Po.Stock" alias="stock"/>
        <typeAlias type="com.imustacm.domain.Po.Storage" alias="storage"/>
        <typeAlias type="com.imustacm.domain.Po.Sales" alias="sales"/>
        <typeAlias type="com.imustacm.domain.Po.Users" alias="users"/>
        <typeAlias type="com.imustacm.domain.Po.Image" alias="image"/>
    </typeAliases>

    <!--映射文件路径-->
    <mappers>
        <package name="com.imustacm.mapper"/>
    </mappers>
</configuration>